<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/habitButton"
        android:drawable="@drawable/empty_habit_button" />
    <item
        android:id="@+id/habitButtonDone"
        android:drawable="@drawable/state_habit_done" />


    <item>
        <shape
            android:shape="ring"
            android:innerRadiusRatio="2"
            android:innerRadius="33dp"
            android:useLevel="false">

            <solid android:color="#001465" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="ring"
            android:innerRadiusRatio="2"
            android:thickness="50dp"
            android:useLevel="false">

            <solid android:color="#ffffff" />
        </shape>
    </item>
</layer-list>